@model List<Trikal_Website.Models.ViewModels.BrochureRequestDetailsViewModel>


<table id="BrochureRequestDetails" class="illusionztable1">
    <thead>
        <tr>
            <th>Request Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Requested On</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var m in Model)
        {
        <tr>
            <td>@m.BrochureRequestId</td>
            <td>@m.Name</td>
            <td>@m.Email</td>
            <td>@m.LastUpdateDate</td>
            <td style="text-align:center;">
                <i class="fa fa-paper-plane admin-action" aria-hidden="true" style="margin-right:7px;color:forestgreen;" BrochureRequestId=@m.BrochureRequestId MarkAsSpam="false" SendBit="true"></i>
                <i class="fa fa-ban admin-action" aria-hidden="true" style="margin-left:7px;color:red;"  BrochureRequestId=@m.BrochureRequestId MarkAsSpam="true" SendBit="false"></i>
            </td>
        </tr>
        }
    </tbody>
</table>